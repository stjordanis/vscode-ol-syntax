{
  "name": "vscode-ol-syntax",
  "displayName": "Ol (Scheme) Syntax Highlight",
  "description": "Otus Lisp (Scheme, Lisp) VSCode Syntax Highlight",
  "version": "0.1.39",
  "publisher": "yuriy-chumak",

  "repository": "https://github.com/yuriy-chumak/vscode-ol-syntax",
  "license": "WTFPL",

  "author": {
    "name": "Yuriy Chumak (a.k.a uri)"
  },
  "icon": "images/ol.png",
  "categories": ["Programming Languages"],

  "private": false,
  "dependencies": {},
  "devDependencies": {
    "typescript": "^1.6.2",
    "vscode": "0.10.x"
  },
  "engines": {
    "vscode": "^0.10.10"
  },
  "contributes": {
    "languages": [{
      "id": "ol (scheme)",
      "aliases": ["Lisp", "lisp", "Scheme", "scheme", "ol"],
      "extensions": [".lisp", ".lsp", ".ol", ".scm"],
      "configuration": "./ol.configuration.json"
    }],
    "grammars": [{
      "language": "ol (scheme)",
      "scopeName": "source.lisp",
      "path": "./Lisp/ol.tmLanguage"
    }]
  },
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/yuriy-chumak/vscode-ol-syntax/issues"
  }
}
